# platform :ios, '9.0'

target 'FridgeWatch' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for FridgeWatch
  platform :ios, '9.0'
  pod 'PPBlinkOCR', '~> 4.0.0'

  target 'FridgeWatchTests' do
    inherit! :search_paths
    # Pods for testing
    platform :ios, '9.0'
    pod 'PPBlinkOCR', '~> 4.0.0'

  end

  target 'FridgeWatchUITests' do
    inherit! :search_paths
    # Pods for testing
    platform :ios, '9.0'
    pod 'PPBlinkOCR', '~> 4.0.0'

  end

end
